{
  "manifest_version":2,
  "permissions": [
    "activeTab",
    "tabs",
    "webRequest",
    "contextMenus",
    "nativeMessaging"
  ],
  "version":"1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "name":"Test",
  "background":{
    "page": "background.html",
    "persistent": false
    },
  "content_scripts":[
    {
     "matches":["<all_urls>"],
     "js":["main.js"]
    }
  ]
}

